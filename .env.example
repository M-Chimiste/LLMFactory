# LLMFactory Environment Variables Configuration
# Copy this file to .env and fill in your actual API keys and configuration

# Anthropic API
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# OpenAI API
OPENAI_API_KEY=your-openai-api-key-here

# Google Gemini API
GOOGLE_API_KEY=your-google-api-key-here

# Ollama Configuration
OLLAMA_URL=http://localhost:11434

# LM Studio Configuration
LMSTUDIO_HOST=localhost:1234

# AWS Bedrock Configuration
AWS_PROFILE=your-aws-profile-name
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_SESSION_TOKEN=your-aws-session-token

# Custom OpenAI-Compatible API
CUSTOM_OAI_BASE_URL=http://localhost:8000
CUSTOM_OAI_API_KEY=your-custom-api-key
